/* components/canlendar/canlendar.wxss */
.calendar-title{
    display: flex;
    width: 100%;
    height: 60rpx;
    
}
.item-ctrl{
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    margin-left: 50rpx;

}
.calendar-week{
    display: flex;
    text-align: center;
    padding: 20rpx 10rpx;
    border-bottom: 1rpx solid #e0e0e0;
    border-top: 1rpx solid #e0e0e0;
    background: #f5f5f5;
}
.calendar-week .item{
    flex: 1;
}
